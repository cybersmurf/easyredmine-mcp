    "typescript": "^5.6.3",
    "eslint": "^9.14.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "prettier": "^3.3.3",
    "eslint-config-prettier": "^9.1.0"
